\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthemekami}[2016/03/14 kami color theme]
\RequirePackage{pgfopts}
\pgfkeys{
  /kami/color/block/.cd,
    .is choice,
    transparent/.code=\kami@block@transparent,
    fill/.code=\kami@block@fill,
}
\pgfkeys{
  /kami/color/background/.cd,
    .is choice,
    dark/.code=\kami@colors@dark,
    light/.code=\kami@colors@light,
}
\newcommand{\kami@color@setdefaults}{
  \pgfkeys{/kami/color/.cd,
    background=light,
    block=transparent,
  }
}
% \definecolor{mDarkBrown}{HTML}{604C38}
\definecolor{mDarkTeal}{HTML}{21252B}%#21252B
% \definecolor{kColAlerted}{HTML}{b6f442}
\definecolor{kColAlerted}{HTML}{FFFC31}%#FFFC31
\definecolor{kColExample}{HTML}{FFFC31}%#FFFC31
\newcommand{\kami@colors@dark}{
  \setbeamercolor{normal text}{%
    fg=white,
    bg=mDarkTeal
  }
}
\newcommand{\kami@colors@light}{
  \setbeamercolor{normal text}{%
    fg=mDarkTeal,
    bg=white
  }
}
\setbeamercolor{alerted text}{%
  % fg=kColAlerted,
  fg=mDarkTeal,
  bg=kColAlerted
}
\newcommand{\boxalert}[1]{{%
  \usebeamercolor{alerted text}\colorbox{bg}{\color{fg}{#1}}%
}}
\setbeamercolor{example text}{%
  fg=kColExample
}
\setbeamercolor{titlelike}{use=normal text, parent=normal text}
\setbeamercolor{author}{use=normal text, parent=normal text}
\setbeamercolor{date}{use=normal text, parent=normal text}
\setbeamercolor{institute}{use=normal text, parent=normal text}
\setbeamercolor{structure}{use=normal text, fg=normal text.fg}
\setbeamercolor{palette primary}{%
  use=normal text,
  fg=normal text.fg,
  bg=normal text.bg
}
\setbeamercolor{frametitle}{%
  use=palette primary,
  parent=palette primary
}
\setbeamercolor{progress bar}{%
  use=normal text,
  fg=normal text.fg,
  bg=normal text.bg
  % bg=normal text.fg!50!black!30
}
\setbeamercolor{title separator}{
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in head/foot}{%
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in section page}{
  use=progress bar,
  parent=progress bar
}
\newcommand{\kami@block@transparent}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=
  }
  \setbeamercolor{block body}{
    bg=
  }
}
\newcommand{\kami@block@fill}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=normal text.bg!80!fg
  }
  \setbeamercolor{block body}{
    use={block title, normal text},
    bg=block title.bg!50!normal text.bg
  }
}
\setbeamercolor{block title alerted}{%
    use={block title, alerted text},
    bg=block title.bg,
    fg=alerted text.fg
}
\setbeamercolor{block title example}{%
    use={block title, example text},
    bg=block title.bg,
    fg=example text.fg
}
\setbeamercolor{block body alerted}{use=block body, parent=block body}
\setbeamercolor{block body example}{use=block body, parent=block body}
\setbeamercolor{footnote}{fg=normal text.fg!90}
\setbeamercolor{footnote mark}{fg=.}
\kami@color@setdefaults
\ProcessPgfPackageOptions{/kami/color}
\mode<all>
\endinput
